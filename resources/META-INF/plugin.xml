<idea-plugin>
    <id>com.enums.pulgin</id>
    <name>Enum-Generator</name>
    <version>1.0</version>
    <vendor email="jlj_Vip98@qq.com" url="https://github.com/jiang95/Enum-Generator-Plugin">蒋先森</vendor>

    <description><![CDATA[
      To init java Enum and generator valueOfXXX method;
      在枚举方法类中使用 Mac control + enter 或者 windows alt+ insert， 选择 EnumGenerator，生成枚举方法。
    ]]></description>

    <change-notes><![CDATA[
       To init java Enum and generator valueOfXXX method;
      在枚举方法类中使用 Mac control + enter 或者 windows alt+ insert， 选择 EnumGenerator，生成枚举方法。
    ]]>
    </change-notes>

    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
    <idea-version since-build="173.0"/>

    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
         on how to target different products -->
    <!-- uncomment to enable plugin in all products
    <depends>com.intellij.modules.lang</depends>
    -->

    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
    </extensions>

    <actions>
        <!-- Add your actions here -->
        <action id="EnumGenerator" class="com.enums.pulgin.EnumGenerator" text="EnumGenerator"
                description="Enum Generator">
            <add-to-group group-id="GenerateGroup" anchor="first"/>
        </action>
    </actions>

</idea-plugin>